import java.util.Arrays;

public class TwoDMatrix {

    public static void main(String[] args) {
        // Create a 2D matrix with 3 rows and 3 columns
        int[][] matrix = {
            {1, 2, 3},
            {4, 5, 6},
            {7, 8, 9}
        };
        
        // Print the 2D matrix
        System.out.println("2D Matrix:");
        for (int[] row : matrix) {
            System.out.println(Arrays.toString(row));
        }
    }
}
