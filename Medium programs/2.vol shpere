import java.util.Scanner;
public class sphere{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("enter a number:");
        int r=sc.nextInt();
        double pi=3.14;
        double vol=1.33333*3.14*r*r*r;
        System.out.println("the vol of sphere is :"+vol);
        }
}
